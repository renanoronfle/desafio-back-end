<p id="notice"><%= notice %></p>

<h1>Cnab</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cnabs.each do |cnab| %>
      <tr>
        <td><%= cnab.name %></td>
        <td><%= link_to 'Show', cnab %></td>
        <td><%= link_to 'Edit', edit_cnab_path(cnab) %></td>
        <td><%= link_to 'Destroy', cnab, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New cnab', new_cnab_path %>
